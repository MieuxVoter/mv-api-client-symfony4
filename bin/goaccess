#!/usr/bin/env sh

# This relies heavily on the docker-compose config.
# It's a thin wrapper for the CRON job.

# Go to project dir
SCRIPT_DIR=$(cd -P -- "$(dirname -- "$0")" && pwd -P)
cd "${SCRIPT_DIR}"
cd ..

echo "Working in $(pwd)"

docker-compose up goaccess

echo "Done generating analytics with goaccess."
